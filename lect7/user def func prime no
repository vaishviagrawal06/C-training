#include <stdio.h>
int prime(int n){
    if(n<=1){
        return 0;
    }
    for(int i=2;i*i<=n;i++){
        if(n%i==0){
            return 0;
        }
    }
return 1;
}


int main() {
    int x;
    printf("enter a number");
    scanf("%d",&x);
    if(prime(x)){
        printf("%d is prime no.",x);
    }else{
        printf("not a prime no.");
    }
    

    return 0;

}
